{
    "Comment": "Executing two lambda functions consecutive",
    "StartAt": "LambdaA",
    "States": {
        "LambdaA": {
            "Type": "Task",
            "Resource": "arn:aws:states:::lambda:invoke",
            "Parameters": {
                "FunctionName.$": "$.LambdaAArn",
                "Payload.$": "$.InputData"
            },
            "ResultPath": "$.LambdaAResult",
            "Next": "LambdaB"
        },
        "LambdaB": {
            "Type": "Task",
            "Resource": "arn:aws:states:::lambda:invoke",
            "Parameters": {
                "FunctionName.$": "$.LambdaBArn",
                "Payload": {
                    "fromA.$": "$.LambdaAResult.Payload",
                    "event.$": "$.InputData"
                }
            },
            "OutputPath": "$.Payload",
            "End": true
        }
    }
}